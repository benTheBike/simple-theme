<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="theme-color" content="<%= theme.accent_color %>">
    <meta name="description" content="<%= config.description %>">
    <meta name="subject" content="<%= config.title %>">
    <title><%= config.title %></title>
    <link href="https://necolas.github.io/normalize.css/8.0.0/normalize.css" type="text/css" rel="stylesheet">
    <link href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" rel="stylesheet" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp"  crossorigin="anonymous">
    <% if (theme.hljs_css) { %>
        <link href="<%= theme.hljs_css %>" type="text/css" rel="stylesheet">
    <% } else { %>
        <link href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/default.css" type="text/css" rel="stylesheet">
    <% } %>
    <%- css(["css/styles.css"]) %>
</head>